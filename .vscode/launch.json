  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug basic_join",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/build-debug/frontend/basic_join",
      "args": [
        "--dram_gib=1",
        "--vi=false",
        "--mv=false",
        "--isolation_level=ser",
        "--optimistic_scan=false",
        "--pp_threads=1",
        "--csv_truncate=false",
        "--worker_threads=2",
        "--trunc=true",
        "--ssd_path=/mnt/hdd/tmp/test_image",
        "--tpch_scale_factor=10",
        "--csv_path=build-debug/basic_join"
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "initCommands": [
        "command source .lldbinit"
      ]
    },
    {
      "name": "Debug basic_join_group",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/build-debug/frontend/basic_join_group",
      "args": [
        "--dram_gib=1",
        "--vi=false",
        "--mv=false",
        "--isolation_level=ser",
        "--optimistic_scan=false",
        "--pp_threads=1",
        "--csv_truncate=false",
        "--worker_threads=2",
        "--trunc=true",
        "--ssd_path=/mnt/hdd/leanstore_images/basic_join_group/10.image",
        "--recover_file=build-debug/basic_join_group/10.json",
        "--tpch_scale_factor=10",
        "--csv_path=build-debug/basic_join_group/10-in-1.csv"
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "initCommands": [
        "command source .lldbinit"
      ]
    },
    {
      "name": "Debug basic_group",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/build-debug/frontend/basic_group_variant",
      "args": [
        "--dram_gib=1",
        "--vi=false",
        "--mv=false",
        "--isolation_level=ser",
        "--optimistic_scan=false",
        "--pp_threads=1",
        "--csv_truncate=false",
        "--worker_threads=2",
        "--trunc=true",
        "--ssd_path=/mnt/hdd/tmp/test_image",
        "--tpch_scale_factor=10",
        "--csv_path=build-debug/basic_group"
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "initCommands": [
        "command source .lldbinit"
      ]
    },
    {
      "name": "Debug disk-based basic_group",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/build-debug/frontend/basic_group_variant",
      "args": [
        "--dram_gib=0.02",
        "--vi=false",
        "--mv=false",
        "--isolation_level=ser",
        "--optimistic_scan=false",
        "--pp_threads=1",
        "--csv_truncate=false",
        "--worker_threads=2",
        "--trunc=true",
        "--ssd_path=/mnt/hdd/tmp/test_image",
        "--tpch_scale_factor=10",
        "--csv_path=build-debug/basic_group"
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "initCommands": [
        "command source .lldbinit"
      ]
    }
  ]
}